ingredients = 3
ingredients_weight = 100
total = (ingredients * ingredients_weight)
passata = 4.1
worcestershire_sauce = 18.8
maple_syrup = 81.1
total_ingredients = (passata + worcestershire_sauce + maple_syrup)
print (total_ingredients)
print (total)
percentage_sugar = (total_ingredients / total) * 100
print(percentage_sugar)
